{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "commonjs",
        "target": "esnext",
        "strict": true,
        "declaration": true,
        "esModuleInterop": true,
        "allowJs": true,
        "removeComments": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "skipLibCheck": true
    },
    "include": [
        "prisma/**/*.ts",
        "src/**/*",
        "routes/**/*.ts"
    ],
}
